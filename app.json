{
  "expo": {
    "name": "TrusteesVoting",
    "slug": "trustees-voting-mobile-application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#04042c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/106bfef4-5711-45b9-aaee-bd012b52477b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.loyal_codes.trusteesvotingmobileapplication"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "106bfef4-5711-45b9-aaee-bd012b52477b"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  },
  "plugins": [
    [
      "expo-document-picker",
      {
        "iCloudContainerEnvironment": "Production"
      },
      "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
      }

    ]
  ]
}
